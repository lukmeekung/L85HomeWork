<Window x:Class="HashTaBale.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashTaBale"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Height="41" Margin="92,242,0,0" VerticalAlignment="Top" Width="144" Click="AddBtn_Click"/>
        <Button x:Name="removeBtn" Content="Remove" HorizontalAlignment="Left" Height="41" Margin="272,242,0,0" VerticalAlignment="Top" Width="144" Click="RemoveBtn_Click"/>
        <Button x:Name="showAllBtn" Content="Show all" HorizontalAlignment="Left" Height="41" Margin="465,242,0,0" VerticalAlignment="Top" Width="144" Click="ShowAllBtn_Click"/>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="33" Margin="92,30,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="517"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="33" Margin="92,90,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="517"/>

    </Grid>
</Window>
